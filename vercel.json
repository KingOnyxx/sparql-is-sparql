{
    "builds": [
      {
        "src": "Airports/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" , "runtime": "python3.13", "buildCommand": "pip install -r requirements.txt" }

      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "Airports/wsgi.py"
      }
    ]
  }